    "dev": "NODE_ENV=local nodemon app.js",
    "prod": "NODE_ENV=production node app.js",
    "start": "GRPC_NO_SSL=true node app.js",
    "test": "NODE_ENV=test npm run lint && npm run unit-test && echo 'Done.'",
    "lint": "./node_modules/eslint/bin/eslint.js . --max-warnings=0 --report-unused-disable-directives --fix && echo 'âœ”  Your .js files look good.'",
    "unit-test": "ENCRYPTION_KEY=123 NODE_ENV=test ./node_modules/mocha/bin/mocha \"test/unit/**/*.test.js\"",
    "integration-test": "./node_modules/mocha/bin/mocha --timeout=30000 test/lifecycle.test.js \"test/integration/**/*.test.js\"",
    "test-all": "npm run lint && npm run unit-test && npm run integration-test && echo 'Done.'",
    "test-coverage": "nyc npm run test-all",
    "integration-test-coverage": "nyc npm run integration-test",
    "unit-test-coverage": "nyc npm run unit-test",
    "e2e-test": "NODE_ENV=testing node app.js",
    "dev-with-test-db": "NODE_ENV=localTesting node test/loadTestDb.js up && nodemon app.js",
    "destroy-test-db": "NODE_ENV=localTesting node test/loadTestDb.js down"
  },
